<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Toolbar">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="Toolbar.Subtitle">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Textview20dp">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Heading">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="Textview20dp.sub">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Textview20dp.subblack">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">16sp</item>
    </style>


    <style name="miniText14">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="miniText14.abu">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="miniText14.green">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#1ABC9C</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="Textview20dp.namamakan">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="RatingBar">
        <item name="colorControlNormal">#ECECEC</item>
        <item name="colorControlActivated">#FFC700</item>
    </style>

    <style name="Tab">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:gravity">left</item>
    </style>
</resources>